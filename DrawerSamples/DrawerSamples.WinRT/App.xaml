<Intersoft:Application
    x:Class="DrawerSamples.WinRT.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:DrawerSamples.WinRT"
    xmlns:Intersoft="using:Intersoft.Crosslight.WinRT"
    xmlns:IntersoftUI="using:Intersoft.Crosslight.UI.WinRT">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <!-- 
                    Styles that define common aspects of the platform look and feel
                    Required by Visual Studio project and item templates
                 -->
                <ResourceDictionary Source="Assets/Styles/AppStyles.xaml"/>
                <ResourceDictionary Source="Assets/Styles/StandardStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <!-- Application-specific resources -->
            <x:String x:Key="AppName">Crosslight App</x:String>

         

        </ResourceDictionary>
    </Application.Resources>
</Intersoft:Application>
